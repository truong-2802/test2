@model test2.Models.Employee_TblModel;
@{
}
<div class="container">
    @using (Html.BeginForm(FormMethod.Post))
    { // "action" , "controller "  thêm vào trc For...
        @Html.AntiForgeryToken()
        <div class="mb-3">
            @Html.LabelFor(m => m.Employee_name)
            @Html.TextBoxFor(m => m.Employee_name)
            @Html.LabelFor(m => m.Employee_code)
            @Html.TextBoxFor(m => m.Employee_code)
            @Html.LabelFor(m => m.Department)
            @Html.TextBoxFor(m => m.Department)
            @Html.LabelFor(m => m.Rank)
            @Html.TextBoxFor(m => m.Rank)
            
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    }
</div>